@model MusicTahp.Models.MusicGenreViewModel@{    ViewData["Title"] = "Đơn Hàng";}<link rel="stylesheet" href="~/css/style.css" /><h1>Đơn hàng</h1><p>    <a asp-action="Create">Tạo mới đơn hàng</a></p><form asp-controller="Musics" asp-action="Index" method="get">    <p>        <select asp-for="MusicGenre" asp-items="Model.Genres">            <option value="">Tất cả</option>        </select>        Tên ca khúc: <input type="text" name="SearchString" />        <input type="submit" value="Tìm" />    </p></form><table class="table">    <thead>        <tr>            <th>                @Html.DisplayNameFor(model => model.Musics[0].TenCaKhuc)            </th>            <th>                @Html.DisplayNameFor(model => model.Musics[0].CaSi)            </th>            <th>                @Html.DisplayNameFor(model => model.Musics[0].TheLoai)            </th>            <th>                @Html.DisplayNameFor(model => model.Musics[0].NgayBan)            </th>            <th>                @Html.DisplayNameFor(model => model.Musics[0].Gia)            </th>            <th>                @Html.DisplayNameFor(model => model.Musics[0].DiaChi)            </th>            <th>                @Html.DisplayNameFor(model => model.Musics[0].TrangThai)            </th>            <th></th>        </tr>    </thead>    <tbody>        @foreach (var item in Model.Musics)        {        <tr>            <td>                @Html.DisplayFor(modelItem => item.TenCaKhuc)            </td>            <td>                @Html.DisplayFor(modelItem => item.CaSi)            </td>            <td>                @Html.DisplayFor(modelItem => item.TheLoai)            </td>            <td>                @Html.DisplayFor(modelItem => item.NgayBan)            </td>            <td>                @Html.DisplayFor(modelItem => item.Gia)            </td>            <td>                @Html.DisplayFor(modelItem => item.DiaChi)            </td>            <td>                @Html.DisplayFor(modelItem => item.TrangThai)            </td>            <td>                <a class="edit" asp-action="Edit" asp-route-id="@item.Id">Chỉnh sửa</a> |                <a class="detail" asp-action="Details" asp-route-id="@item.Id">Chi tiết</a> |                <a class="detele" asp-action="Delete" asp-route-id="@item.Id">Xóa</a>            </td>        </tr>        }    </tbody></table>